{
  "name": "modvault",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "clean": "rm -rf .next node_modules/.cache",
    "dev": "next dev",
    "dev:clean": "npm run clean && next dev",
    "build": "next build",
    "vercel-build": "prisma generate && prisma migrate deploy && next build",
    "postinstall": "prisma generate",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest",
    "test:run": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "tsx scripts/init-db.ts",
    "db:seed:affiliates": "tsx prisma/seed-affiliates.ts",
    "db:seed:facets": "tsx scripts/seed-facet-definitions.ts",
    "db:reset": "prisma migrate reset",
    "db:deploy": "prisma migrate deploy",
    "content:aggregate": "tsx -e \"import { contentAggregator } from './lib/services/contentAggregator'; contentAggregator.runAggregation();\"",
    "content:privacy": "tsx scripts/run-privacy-aggregation.ts",
    "content:stealth": "PRIVACY_LEVEL=stealth tsx scripts/run-privacy-aggregation.ts",
    "content:conservative": "PRIVACY_LEVEL=conservative tsx scripts/run-privacy-aggregation.ts",
    "content:test": "tsx scripts/test-privacy-aggregator.ts",
    "scrape:mhm": "tsx scripts/scrape-musthavemods.ts",
    "scrape:mhm:prod": "NODE_ENV=production tsx scripts/scrape-musthavemods.ts",
    "scrape:wewantmods": "tsx scripts/run-wewantmods-scraper.ts",
    "scrape:wewantmods:stealth": "PRIVACY_LEVEL=stealth tsx scripts/run-wewantmods-scraper.ts",
    "scrape:wewantmods:conservative": "PRIVACY_LEVEL=conservative tsx scripts/run-wewantmods-scraper.ts",
    "scrape:wewantmods:test": "tsx scripts/run-wewantmods-scraper.ts --dry-run --limit=5",
    "fix:descriptions": "tsx scripts/fix-mod-descriptions.ts",
    "fix:descriptions:apply": "tsx scripts/fix-mod-descriptions.ts --fix",
    "mods:verify": "tsx scripts/verify-mods.ts",
    "mods:verify:apply": "tsx scripts/verify-mods.ts --verify",
    "update:images": "tsx scripts/update-missing-images.ts",
    "update:images:prod": "NODE_ENV=production tsx scripts/update-missing-images.ts",
    "ratings:recalculate": "tsx scripts/recalculate-ratings.ts",
    "categories:migrate": "tsx scripts/migrate-categories.ts",
    "images:migrate": "tsx scripts/migrate-image-urls.ts",
    "populate:creators": "tsx scripts/populateCreators.ts",
    "admin:create": "tsx scripts/create-admin.ts",
    "type-check": "tsc --noEmit",
    "facets:vision": "tsx scripts/migrate-with-vision.ts",
    "facets:vision:test": "tsx scripts/migrate-with-vision.ts --test",
    "facets:vision:text": "tsx scripts/migrate-with-vision.ts --text-only",
    "facets:vision:clear": "tsx scripts/migrate-with-vision.ts --clear",
    "facets:check": "tsx scripts/check-facets.ts",
    "facets:audit": "tsx scripts/ralph/facet-audit.ts",
    "screenshot": "tsx scripts/ralph/screenshot.ts",
    "env:push": "node scripts/sync-env-to-vercel.js",
    "env:pull": "vercel env pull .env.production",
    "env:list": "vercel env ls",
    "queue": "tsx scripts/manage-queue.ts",
    "agent": "tsx scripts/run-agent.ts",
    "agent:full": "tsx scripts/run-agent.ts full",
    "agent:ga4-sync": "tsx scripts/run-agent.ts ga4_sync",
    "agent:mediavine-sync": "tsx scripts/run-agent.ts mediavine_sync",
    "agent:scan-affiliates": "tsx scripts/run-agent.ts affiliate_scan",
    "agent:analyze-rpm": "tsx scripts/run-agent.ts rpm_analysis",
    "agent:forecast": "tsx scripts/run-agent.ts forecast",
    "agent:cleanup": "tsx scripts/run-agent.ts cleanup",
    "agent:report": "tsx scripts/run-agent.ts report",
    "agent:mediavine-csv": "tsx scripts/sync-mediavine-metrics.ts csv",
    "security:check-admin-auth": "tsx scripts/security/check-admin-auth.ts"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^1.0.12",
    "@google-analytics/data": "^5.2.1",
    "@marsidev/react-turnstile": "^1.3.1",
    "@prisma/client": "^6.15.0",
    "@prisma/extension-accelerate": "^3.0.1",
    "@stripe/stripe-js": "^8.5.3",
    "@tailwindcss/typography": "^0.5.19",
    "@upstash/redis": "^1.35.6",
    "@vercel/analytics": "^1.6.1",
    "@vercel/blob": "^2.0.0",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "cheerio": "^1.0.0-rc.12",
    "framer-motion": "^10.16.5",
    "https-proxy-agent": "^7.0.6",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.294.0",
    "next": "14.0.0",
    "next-auth": "^4.24.5",
    "openai": "^4.20.1",
    "otpauth": "^9.4.1",
    "react": "^18",
    "react-dom": "^18",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1",
    "socks-proxy-agent": "^8.0.5",
    "stripe": "^20.0.0",
    "uuid": "^13.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/coverage-v8": "^4.0.16",
    "autoprefixer": "^10.4.16",
    "eslint": "^8",
    "eslint-config-next": "14.0.0",
    "jsdom": "^27.3.0",
    "playwright": "^1.57.0",
    "postcss": "^8.4.32",
    "prisma": "^6.15.0",
    "tailwindcss": "^3.3.0",
    "tsx": "^4.6.2",
    "typescript": "^5",
    "vitest": "^4.0.16"
  }
}
