{
  "name": "modvault",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "vercel-build": "prisma generate && prisma migrate deploy && next build",
    "postinstall": "prisma generate",
    "start": "next start",
    "lint": "next lint",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "tsx scripts/init-db.ts",
    "db:reset": "prisma migrate reset",
    "db:deploy": "prisma migrate deploy",
    "content:aggregate": "tsx -e \"import { contentAggregator } from './lib/services/contentAggregator'; contentAggregator.runAggregation();\"",
    "content:privacy": "tsx scripts/run-privacy-aggregation.ts",
    "content:stealth": "PRIVACY_LEVEL=stealth tsx scripts/run-privacy-aggregation.ts",
    "content:conservative": "PRIVACY_LEVEL=conservative tsx scripts/run-privacy-aggregation.ts",
    "content:test": "tsx scripts/test-privacy-aggregator.ts",
    "scrape:mhm": "tsx scripts/scrape-musthavemods.ts",
    "scrape:mhm:prod": "NODE_ENV=production tsx scripts/scrape-musthavemods.ts",
    "update:images": "tsx scripts/update-missing-images.ts",
    "update:images:prod": "NODE_ENV=production tsx scripts/update-missing-images.ts",
    "ratings:recalculate": "tsx scripts/recalculate-ratings.ts",
    "categories:migrate": "tsx scripts/migrate-categories.ts",
    "populate:creators": "tsx scripts/populateCreators.ts",
    "admin:create": "tsx scripts/create-admin.ts",
    "type-check": "tsc --noEmit",
    "env:push": "node scripts/sync-env-to-vercel.js",
    "env:pull": "vercel env pull .env.production",
    "env:list": "vercel env ls"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^1.0.12",
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "@marsidev/react-turnstile": "^1.3.1",
    "@prisma/client": "^6.15.0",
    "@stripe/stripe-js": "^8.5.3",
    "@upstash/redis": "^1.35.6",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "build": "^0.1.4",
    "cheerio": "^1.0.0-rc.12",
    "framer-motion": "^10.16.5",
    "https-proxy-agent": "^7.0.6",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.294.0",
    "next": "14.0.0",
    "next-auth": "^4.24.5",
    "openai": "^4.20.1",
    "react": "^18",
    "react-dom": "^18",
    "socks-proxy-agent": "^8.0.5",
    "stripe": "^20.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.4.16",
    "eslint": "^8",
    "eslint-config-next": "14.0.0",
    "postcss": "^8.4.32",
    "prisma": "^6.15.0",
    "tailwindcss": "^3.3.0",
    "tsx": "^4.6.2",
    "typescript": "^5"
  }
}
